{
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "prettier.configPath": ".prettierrc",
  "cSpell.words": [
    "antd",
    "Avenir",
    "CHAKRA",
    "mantine",
    "optimizelegibility",
    "shadcn",
    "stylelint",
    "typecheck",
    "untoggled",
    "zustand"
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#f74e5a",
    "activityBar.background": "#f74e5a",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#1cf40b",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#e7e7e799",
    "sash.hoverBorder": "#f74e5a",
    "statusBar.background": "#f51d2c",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#f74e5a",
    "statusBarItem.remoteBackground": "#f51d2c",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#f51d2c",
    "titleBar.activeForeground": "#e7e7e7",
    "titleBar.inactiveBackground": "#f51d2c99",
    "titleBar.inactiveForeground": "#e7e7e799"
  },
  "peacock.color": "#F51D2C",
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "typescript.tsdk": "node_modules/typescript/lib"
}
